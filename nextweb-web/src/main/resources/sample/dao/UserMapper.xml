<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sample.dao.UserDAO">
  <select id="findAll" resultType="cn.corp.common.data.User">
    select * from user
  </select>
  
  <select id="findById" resultType="cn.corp.common.data.User">
    select * from user where id=#{VALUE}
  </select>
</mapper>

